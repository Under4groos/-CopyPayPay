<Window x:Class="CopyPayPay.Contrlos.WinText"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CopyPayPay.Contrlos"
        mc:Ignorable="d"
        Title="WinText" Height="617" Width="1164" ShowInTaskbar="True" WindowStartupLocation="CenterScreen">
    <Grid>
        <RichTextBox  x:Name="RichTextBox_" Grid.ColumnSpan="2" Background="{x:Null}" BorderBrush="{x:Null}" SelectionTextBrush="{x:Null}" Foreground="Black">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>

    </Grid>
</Window>
